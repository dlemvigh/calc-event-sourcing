generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CalcJobResult {
  id     String  @id @db.Uuid
  input  BigInt
  output BigInt?
}

model CalcJobStatus {
  id         String    @id @db.Uuid
  createdAt  DateTime  @default(now())
  startedAt  DateTime?
  finishedAt DateTime?
}
